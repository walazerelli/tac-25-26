import nbformat

input_file = "TP2/ton_notebook_corrompu.ipynb"  # Remplace par le vrai nom du notebook
output_file = "TP2/ton_notebook_fixed.ipynb"    # Nom du nouveau notebook réparé

with open(input_file) as f:
    nb = nbformat.read(f, as_version=4)
with open(output_file, "w") as f:
    nbformat.write(nb, f)

print(f"Notebook réparé et sauvegardé sous {output_file}")




